                <h2 class="h"> المنتجات</h2>
                <div class="section_0 d-flex overscrol ">
                    {{#each product}}
                    <div class="ss">
                        <div class="text-center">
                            <img src="{{this.avatar}}" style="width: 150px;height: 150px;border-radius: 50%;">
