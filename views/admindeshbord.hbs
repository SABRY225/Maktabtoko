    <style>
        section {
            padding: 2rem 5%;
        }

        .overscrol {
            width: auto;
            /* Set a fixed width for the container */
            white-space: nowrap;
            overflow: auto;
            /* scroll thumb and track */
        }

        .overscrol::-webkit-scrollbar {
            width: 2px;

        }

        .overscrol::-webkit-scrollbar-thumb {

            border-radius: 10px;
            background-color: #d4af37;
            /* Set thumb color for Chrome and Safari */
        }

        .overscrol::-webkit-scrollbar-track {
            width: 2px;
            background-color: rgba(0, 128, 0, 0);
            /* Set track color for Chrome and Safari */
        }

        .ss {
            border: 1px solid rgb(243, 235, 235);
            padding: 50px;

            z-index: 101;
            background-color: rgba(216, 216, 216, 0.263);
            box-shadow: 0px 0px 10px #ffffff;

            border-radius: 10px;
            margin: 50px 10px 10px 10px;
            color: #ffffff;
        }

        .inputs-details h3,
        .inputs-details h4 {
            border: 2px solid rgba(255, 255, 255, 0.447);
            border-radius: 10px;
            padding: 10px;
            margin-top: 10px;
            font-size: 16px;
            max-width: 300px;
            white-space: normal;
            overflow-wrap: break-word;
            max-height: 80px;
            overflow-y: auto;
        }

        .overscoinput::-webkit-scrollbar {
            width: 8px;
        }

        .overscoinput::-webkit-scrollbar-thumb {

            border-radius: 10px;
            background-color: #d4af37;
            /* Set thumb color for Chrome and Safari */
        }

        .overscoinput::-webkit-scrollbar-track {
            width: 2px;
            background-color: rgba(0, 128, 0, 0);
            /* Set track color for Chrome and Safari */
        }


        .header_section_4_2 {
            margin-top: 70px;
        }

        .header_section_5,
        .all {
            height: 380vh;
        }

        .header_section_4_1 {
            margin-top: 550px;
        }

        .cardacadame {
            max-width: 400px;
            white-space: normal;
            overflow-wrap: break-word;
            max-height: 370px;
            background-color: rgba(216, 216, 216, 0.263);
            box-shadow: 0px 0px 10px #ffffff;
            font-family: inherit;
            position: relative;
            border-radius: 10px;
            padding: 10px;
        }

        .quotecardacadame {
            color: rgb(203, 6, 6);
            padding-left: 20px;
            position: relative;
        }

        .cardacadame-name {
            font-weight: 700;
            color: rgb(0, 0, 0);
            padding: 35px;
            line-height: 23px;
        }

